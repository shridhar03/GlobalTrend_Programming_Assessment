class HashMap {
  constructor() {
    this.map = {};
  }
  put(key, value) {
    this.map[key] = value;
  }
  get(key) {
    return this.map[key];
  }
  remove(key) {
    if (this.map.hasOwnProperty(key)) {
      delete this.map[key];
    }
  }
}
const map = new HashMap();
map.put("key1", "value1");
map.put("key2", "value2");
map.put("key3", "value3");

console.log(map.get("key2")); 

map.remove("key1");
console.log(map.get("key1")); 
